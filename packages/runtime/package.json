{
  "name": "runtime-server",
  "version": "2.0.0",
  "description": "SmythOS Runtime Server (Agent, Debugger, Embodiment)",
  "main": "rollup.config.js",
  "type": "module",
  "scripts": {
    "dev": "npm run build:dev && node ./dist/runtime-server.dev.cjs",
    "build:dev": "cross-env NODE_OPTIONS='--max-old-space-size=4096' BUILD=dev FORMAT=cjs rollup -c ./rollup.config.js",
    "start": "node ./dist/runtime-server.dev.cjs",
    "test": "cross-env TEST_ENV=true vitest --reporter verbose",
    "prepare:staging": "npm version patch --no-git-tag-version",
    "prepare:production": "npm version minor -f",
    "deploy:dev": "git tag -f dev-deploy && git push origin dev-deploy -f"
  },
  "author": "Alaa-eddine KADDOURI",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/compression": "^1.8.1",
    "@types/express-http-proxy": "^1.6.6",
    "@types/node": "^22.9.0",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^9.0.8",
    "cross-env": "^7.0.3",
    "pkg": "^5.8.1",
    "rollup": "^2.79.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.717.0",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@smythos/sre": "^1.5.50",
    "axios": "^1.7.7",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-http-proxy": "^2.1.1",
    "express-session": "^1.18.1",
    "gpt-tokenizer": "^2.8.1",
    "http-proxy-middleware": "^3.0.3",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.72.0",
    "openapi-to-postmanv2": "^4.24.0",
    "pkce-challenge": "^5.0.0",
    "qs": "^6.14.0",
    "rate-limiter-flexible": "^5.0.4",
    "swagger-ui-dist": "^5.18.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  }
}

{
  "name": "smyth-builder",
  "version": "1.54.185",
  "description": "SmytOS UI",
  "scripts": {
    "build:vanilla": "cross-env NODE_OPTIONS='--max-old-space-size=4096' rollup --config ./scripts/build/dev/rollup-vanilla.config.mjs",
    "build:frontend": "cross-env NODE_OPTIONS='--max-old-space-size=4096' rollup --config ./scripts/build/dev/rollup-fe.config.mjs",
    "build:backend": "rollup --config ./scripts/build/dev/rollup-be.config.mjs",
    "build:frontend:prod": "rollup --config ./scripts/build/prod/rollup-fe.config.mjs",
    "build:backend:prod": "rollup --config ./scripts/build/prod/rollup-be.config.mjs",
    "build:css": "tailwindcss build -i src/builder-ui/styles/tailwind-v2.css -o dist/assets/css/tailwind-v2.css -c ./scripts/build/shared/tailwind-v2.config.js",
    "preview": "node ./dist/server/index.js",
    "bundle:prod": "pnpm run build:backend:prod && pnpm run build:css && pnpm run build:frontend:prod",
    "bundle:dev": "pnpm run build:backend && pnpm run build:css && pnpm run build:frontend",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "knip": "knip",
    "prepare:staging": "pnpm version patch --no-git-tag-version",
    "shadcn:add": "shadcn add -o",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:all": "tsc --noEmit --skipLibCheck && tsc --noEmit --skipLibCheck ",
    "dev:react": "cd src/react && pnpm run dev",
    "dev": "concurrently --kill-others-on-fail -n VIEWS,SERVER,BUILDER-UI,REACT -c blue,magenta,green,cyan,magenta \"nodemon --config ./scripts/watch/nodemon-views.json\" \"nodemon --config ./scripts/watch/nodemon-be.json\" \"nodemon --config ./scripts/watch/nodemon-vanilla.json\" \"pnpm run dev:react\""
  },

<div class="flex justify-center items-center h-screen hidden" id="editValuesDialogTpl">
    <div>
        <!-- Modal Overlay -->
        <div class="fixed inset-0 px-2 z-[11] overflow-hidden flex items-center justify-center">
            <div
                class="__overlay absolute inset-0 bg-gray-500 bg-opacity-75 transition-opacity ease-out duration-300 opacity-25">
            </div>
            <!-- Modal Content -->
            <div class="__dialog smyth-dialog p-2 bg-white shadow-xl max-w-2xl w-full max-h-screen z-50 transition-all ease-out duration-300 transform scale-0 border-solid border-white rounded-xl overflow-y-auto scroll-smooth">
                <!-- Modal Header -->
                <div class="text-[#1e1e1e] pt-4 px-4 flex justify-between">
                    <h2 class="__title text-xl font-semibold"></h2>


                    <button class="__btnClose" aria-label="Close modal">
                        <svg class="w-[20px] h-[20px]" fill="none" stroke="currentColor" viewBox="0 0 24 24"
                            xmlns="http://www.w3.org/2000/svg">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                d="M6 18L18 6M6 6l12 12"></path>
                        </svg>
                    </button>

                </div>
                <!-- Modal Body -->
                <div class="__content p-2">
                </div>
                <!-- Modal Footer -->
                <div class="__actions px-4 pb-4 flex justify-end space-x-2">
                    <!-- <button
                        class="__btnCancel px-3 py-1 bg-gray-400 text-white transition-opacity rounded-md w-full sm:w-auto hover:opacity-75">
                        Cancel </button>
                    <button
                        class="__btnAccept px-3 py-1 bg-emerald-500 text-white transition-opacity rounded-md w-full sm:w-auto hover:opacity-75">
                        Save </button> -->
                </div>
            </div>
        </div>
    </div>
</div>


<div data-role="dialog" class="dialog smyth-dialog shadow-on bg-white rounded-md shadow dark:bg-gray-700"
    data-width="600" id="smyth-dialog" data-overlay="false">
    <div class="dialog-title flex items-start justify-between px-2 py-0 border-b rounded-t dark:border-gray-600">Dialog
        Title</div>
    <div class="dialog-content px-2 py-3 max-h-[60vh] overflow-y-auto">Dialog Content</div>
    <div class="dialog-actions flex space-x-4 justify-center items-center py-4">
        <button
            class="text-gray-700 text-center text-sm font-medium py-2 px-3 w-40 rounded-lg border border-gray-700 hover:opacity-75 focus:ring-4 focus:outline-none js-dialog-close btn-cancel">
            Cancel
        </button>

        <button
            class="text-white text-center text-sm font-medium py-2 px-3 w-40 bg-smyth-emerald-400 rounded-lg border border-smyth-emerald-400 hover:opacity-75 focus:ring-4 focus:outline-none btn-submit">
            Save
        </button>
    </div>
</div>

<div class="flex justify-center items-center h-screen hidden" id="tweditValuesDialogTpl">
    <div>
        <!-- Modal Overlay -->
        <div class="fixed inset-0 px-2 z-[11] overflow-hidden flex items-center justify-center">
            <div class="__overlay absolute inset-0 bg-gray-500 bg-opacity-75 transition-opacity ease-out duration-300 opacity-25"></div>
            <!-- Modal Content -->
            <div class="__dialog smyth-dialog bg-white rounded-xl shadow-xl max-w-2xl min-w-[550px] p-2 z-50 transition-all ease-out duration-300 transform scale-0 border-solid border-white border-2 max-h-screen overflow-y-auto scroll-smooth"">
                <!-- Modal Header -->
                <div class="bg-white text-[#1E1E1E] px-4 py-2 flex justify-between items-center">
                    <h2 class="__title text-xl font-semibold"></h2>
                    <button class="__btnClose" aria-label="Close modal">
                        <svg class="w-[20px] h-[20px]" fill="none" stroke="#666" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path>
                        </svg>
                    </button>
                </div>
                <!-- Modal Body -->
                <div class="__content max-h-[60vh] overflow-y-auto scroll-smooth px-4">
                </div>
                <!-- Modal Footer -->
                <div class="__actions px-4 py-2 flex justify-center">
                </div>
            </div>
        </div>
    </div>
</div>


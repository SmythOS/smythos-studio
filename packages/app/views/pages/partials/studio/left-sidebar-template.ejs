    class="flex gap-2 items-center px-3 py-2 text-xs font-medium leading-none text-center text-gray-800 bg-gray-200 rounded-lg animate-pulse dark:bg-gray-900 dark:text-gray-400"
  >
    <span>Loading</span>
    <div role="status">
      <svg
        aria-hidden="true"
        class="inline w-4 h-4 text-white animate-spin fill-gray-950"
        viewBox="0 0 100 101"
        fill="none"
        xmlns="http://www.w3.org/2000/svg"
      >

<div id="root"></div>

<% if (env === 'DEV') { %>
<script type="module" src="/assets/app.bundle.dev.js"></script>
<% } else { %>
<script type="module" src="/assets/app.bundle.min.js"></script>
<% } %>

<script>
  // if localhost print user a red message in console the you need to run npm run dev
  if (window.location.hostname === 'localhost') {
    console.log(
      `%c npm run dev:v2`,
      'background: #bc8686; color: white;font-weight:500;font-size:20px;line-height:40px',
    );
  }
</script>

<script>
  // Observability flag
  window.isObservability = false;
</script>

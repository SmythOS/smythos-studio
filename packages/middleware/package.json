{
  "name": "smyth-middleware",
  "version": "1.4.281",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest --config vitest.config.ts",
    "prepare": "husky install",
    "bump": "pnpm version patch",
    "lint": "eslint --ext .ts src/",
    "ts-error-scan": "tsc --noEmit",
    "prettier:format": "prettier --write **/*.{js,jsx,ts,tsx,json,css,md}",
    "lint-fix": "eslint --ext .ts src/ --fix",
    "lint-staged": "lint-staged",
    "prisma:format:primary": "prisma format ./prisma/schema.prisma",
    "prisma:format:logs": "prisma format ./prisma-logs/schema.prisma",
    "prisma:generate:primary": "prisma generate --schema ./prisma/schema.prisma",
    "prisma:generate:logs": "prisma generate --schema ./prisma-logs/schema.prisma",
    "prisma:generate:all": "pnpm run prisma:generate:primary && pnpm run prisma:generate:logs",
    "prisma:studio": "prisma studio",
    "ai:generate-doc": "node -r esbuild-register _automation/doc-generator.ts",
    "prisma:migrate:primary": "pnpm run pre-migration-check && prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma:migrate:logs": "pnpm run pre-migration-check && prisma migrate dev --schema ./prisma-logs/schema.prisma",
    "prisma:deploy:primary": "pnpm run pre-migration-check && prisma migrate deploy --schema ./prisma/schema.prisma",
    "prisma:deploy:logs": "pnpm run pre-migration-check && prisma migrate deploy --schema ./prisma-logs/schema.prisma",
    "pre-migration-check": "node -r esbuild-register ./src/scripts/_sys/_pre-migrate-check.ts",
    "dev": "nodemon",
    "build": "rollup -c",
    "start:prod": "node -r esbuild-register dist/index.js",
    "script": "node -r esbuild-register",
    "format": "prettier --write \"**/*.ts\"",
    "graph:gendot": "depcruise src --config .dependency-cruiser.cjs --output-type dot > ./doc/dependency.dot",
    "create-module": "bash _automation/commands/create-module.sh",
    "version": "pnpm run build",
    "postinstall": "pnpm run prisma:generate:all"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/async": "^3.2.24",
    "@types/cors": "^2.8.13",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/html-to-text": "^9.0.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.14.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "dependency-cruiser": "^15.4.0",
    "esbuild": "^0.19.11",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "node-mocks-http": "^1.14.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "prisma": "^5.7.1",
    "rollup": "^3.26.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "supertest": "^6.3.3",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.9.5",
    "vitest": "^1.6.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.421.0",
    "@bull-board/api": "^5.9.2",
    "@bull-board/express": "^5.9.2",
    "@date-fns/utc": "^1.2.0",
    "@prisma/client": "^5.7.1",
    "async": "^3.2.5",
    "async-mutex": "^0.5.0",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.2",
    "bullmq": "^5.7.8",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^6.7.0",
    "fs-extra": "^11.1.1",
    "helmet": "^6.1.5",
    "http-status": "^1.6.2",
    "ioredis": "^5.3.2",
    "joi": "^17.9.1",
    "jose": "^4.14.4",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "openai": "^3.2.1",
    "prom-client": "^15.1.2",
    "qs": "^6.11.2",
    "stripe": "^14.12.0",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.9.0"
  }
}

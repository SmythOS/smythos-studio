{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "ES2020",
        "module": "ES2020",
        "esModuleInterop": true,
        "sourceMap": true,
        "outDir": "./static/js",
        "noImplicitAny": false,
        "resolveJsonModule": true,
    },
    "include": ["src/**/*", "node_modules/@types"],
    "exclude": ["node_modules"]
}
